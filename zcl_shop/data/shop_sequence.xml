<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sequence_shop" model="ir.sequence">
            <field name="name">Shop Sequence</field>
            <field name="code">zcl_shop.sequence_shop</field>
            <field name="prefix">SH</field>
            <field name="padding">5</field>
            <field name="number_next_actual">1</field>
            <field name="company_id" eval="False"/>
        </record>



        <record id="shop_transfer" model="ir.sequence">
            <field name="name">Shop Transfer Sequence</field>
            <field name="code">shop.transfer</field>
            <field name="prefix">ST</field>
            <field name="padding">6</field>
            <field name="number_next_actual">1</field>
            <field name="company_id" eval="False"/>
        </record>

  </data>
</odoo>